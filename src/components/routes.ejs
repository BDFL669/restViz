<ul>
  <% routes.forEach(function(route){ %>
  <a
    href="#/home %>"
    onclick="setPath('<%- route.path%>','<%- route.method %>')"
  >
    <li class="route-item">
      <span class="method <%- route.method.toLowerCase() %>"
        ><%- route.method.toUpperCase() %></span
      >
      <span class="path"><%- route.path %></span>

      <span class=""
        ><%- route.description ? route.description : "Not provided!" %></span
      >
    </li>
  </a>
  <% }); %>
</ul>
